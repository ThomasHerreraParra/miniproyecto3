<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.geometry.Insets?>

<VBox spacing="20" alignment="CENTER" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.miniproyecto3.controller.StartController">
    <padding><Insets top="40" bottom="40" left="20" right="20"/></padding>

    <Button text="Jugar" onAction="#onPlayClicked"/>
    <Button text="Instrucciones" onAction="#onInstructionsClicked"/>
    <Button text="Salir" onAction="#onExitClicked"/>
</VBox>
